{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "outDir": "dist",
    "baseUrl": "./",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2018",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
  },
  "types": []
}